{
  "name": "manage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "build": "tsc --build tsconfig.json"
  },
  "bin": {
    "manage": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "JSONStream": "^1.3.5",
    "cli-ux": "^5.4.5",
    "colors": "^1.4.0",
    "event-stream": "^4.0.1",
    "kebab-case": "^1.0.0",
    "lodash": "^4.17.15",
    "pg": "^7.17.1",
    "recursive-readdir": "^2.2.2",
    "rxjs": "^6.5.4",
    "shelljs": "^0.8.3",
    "topojson": "^3.0.2",
    "tslib": "^1.11.0",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/event-stream": "^3.3.34",
    "@types/geojson": "^7946.0.7",
    "@types/jsonstream": "^0.8.30",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.5",
    "@types/recursive-readdir": "^2.2.0",
    "@types/topojson": "^3.2.2",
    "aws-sdk": "^2.616.0",
    "globby": "^11.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.10.0",
    "tslint-immutable": "^6.0.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.2"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "manage",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts"
}

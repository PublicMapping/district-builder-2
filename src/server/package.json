{
  "name": "district-builder-2-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server/src/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "migration:create": "yarn run typeorm migration:create -n",
    "migration:revert": "yarn run typeorm migration:revert",
    "migration:run": "yarn run typeorm migration:run"
  },
  "dependencies": {
    "@drdgvhbh/postgres-error-codes": "0.0.6",
    "@godaddy/terminus": "4.3.1",
    "@nestjs-modules/mailer": "1.4.2",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/serve-static": "2.1.0",
    "@nestjs/swagger": "4.5.4",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/typeorm": "7.0.0",
    "@nestjsx/crud": "4.5.0",
    "@nestjsx/crud-typeorm": "4.5.0",
    "aws-sdk": "2.616.0",
    "base64url": "3.0.1",
    "bcrypt": "4.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "crypto": "1.0.1",
    "lodash": "4.17.15",
    "nodemailer": "6.4.6",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "7.17.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.4",
    "topojson-client": "3.1.0",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.6",
    "@types/nodemailer": "6.4.0",
    "@types/passport-jwt": "3.0.3",
    "@types/supertest": "2.0.8",
    "@types/topojson-client": "3.0.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.10.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.2.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
